!Elotte ezt kell futtatni: xrdb ~/.Xresources

urxvt.termName:        rxvt
urxvt.loginShell:      true
urxvt*urlLauncher:     /usr/bin/chromium
urxvt*perl-lib:        /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher,searchable-scrollback,clipboard
urxvt*font:            xft:Monospace:pixelsize=15
urxvt*boldFont:        xft:Monospace:pixelsize=15
urxvt.scrollBar:       false
urxvt.saveLines:       65535
urxvt.secondaryScroll: true
urxvt.urgentOnBell:    false
urxvt.visualBell:      false
urxvt*geometry:        95x25
urxvt*matcher.button:  1
urxvt*depth:           24
urxvt.cursorColor:     #dcdcdc
urxvt.cursorBlink:     true

urxvt*transparent: true
urxvt*shading: 10

! Reset
*foreground:     #d0d0d0
*background:     #030303
*highlightColor: #870000

! Black + DarkGrey
*color0:  #000000
*color8:  #555555

! DarkRed + Red
*color1:  #AA0000
*color9:  #FF5555

! DarkGreen + Green
*color2:  #00AA00
*color10: #55FF55

! DarkYellow + Yellow
*color3:  #AA5500
*color11: #FFFF55

! DarkBlue + Blue
*color4:  #0000AA
*color12: #5555FF

! DarkMangenta + Mangenta
*color5:  #AA00AA
*color13: #FF55FF

!DarkCyan + Cyan
*color6:  #00AAAA
*color14: #55FFFF

! LightGrey + White
*color7:  #AAAAAA
*color15: #FFFFFF

xrenoise.use_xcursors:     1
xrenoise.display:          :0
xrenoise.display_resource: 0x00,0x09,0x32,0x4C,0x2F,0xED,0xB4,0x57
xrenoise.executable_path:  /usr/bin/renoise
